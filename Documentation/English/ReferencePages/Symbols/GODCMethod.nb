 CellID->271207584,ExpressionUUID->"ad4db81b-d152-4659-8db0-d6b1fb28e627"],

Cell[TextData[{
 "The Fourier transform of  \[Epsilon](",
 StyleBox["r",
  FontWeight->"Bold"],
 ") is calculated and the corresponding matrix is inverted afterwards."
}], "ExampleText",
 CellID->1690238640,ExpressionUUID->"e083c7fa-19f2-46f0-94ed-817bd7d0d049"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"gvec", ",", "eps"}], "}"}], "=", 
   RowBox[{"GTPhPermittivityMatrix", "[", 
    RowBox[{"basvec", ",", "12", ",", "structure", ",", "1", ",", 
     RowBox[{"GODCMethod", "\[Rule]", " ", "\"\<MatrixInverse\>\""}]}], 
    "]"}]}], ";"}]], "Input",
 CellLabel->"In[6]:=",
 CellID->909857320,ExpressionUUID->"dc154dee-c0a3-4866-bff1-56cb04b3d83e"],

Cell["Calculation of the photonic band structure with this method.", \
"ExampleText",
 CellID->751122340,ExpressionUUID->"79aaecb2-0353-4fd7-8b83-c871b30fca7a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"hop", "=", 
   RowBox[{"GTPhMaster", "[", 
    RowBox[{"1", ",", "gvec", ",", "eps", ",", 
     RowBox[{"GOPhPol", "\[Rule]", " ", "\"\<TM\>\""}]}], "]"}]}], ";", 
  RowBox[{"bands2", "=", 
   RowBox[{"GTBands", "[", 
    RowBox[{"hop", ",", "kpts", ",", "10", ",", 
     RowBox[{"GOPhotonic", "\[Rule]", "True"}]}], "]"}]}], ";", 
  RowBox[{"plt2", "=", 
   RowBox[{"GTBandsPlot", "[", 
    RowBox[{"bands2", ",", "8", ",", 
     RowBox[{"FrameLabel", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\"\<\>\"", ",", "\"\<\[Omega] a/(2 \[Pi] c)\>\""}], "}"}]}], 
     ",", 
     RowBox[{
     "PlotLabel", "\[Rule]", "\"\<TM polarization (matrix inverse)\>\""}], 
     ",", 
     RowBox[{"Joined", "\[Rule]", "True"}], ",", 
     RowBox[{"PlotRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "1.70711"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}], ",", 
     RowBox[{"PlotStyle", "\[Rule]", "Red"}]}], "]"}]}]}]], "Input",
 CellLabel->"In[7]:=",
