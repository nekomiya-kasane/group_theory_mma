 RowBox[{
  RowBox[{"ra", "=", "0.2"}], ";", 
  RowBox[{"structure", "=", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"\"\<CircularRod\>\"", ",", "8.9", ",", 
