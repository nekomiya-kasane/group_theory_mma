  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      SubscriptBox["\<\"(ss\[Sigma])\"\>", "2"], 
      RowBox[{"DM", " ", 
       SubscriptBox["\<\"(sp\[Sigma])\"\>", "2"]}], 
