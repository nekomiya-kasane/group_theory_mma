Cell[TextData[{
 "The character of an angular momentum representation matrix for a rotation \
about the z-axis can be calculated by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Chi]", "j"], "(", "\[Alpha]", ")"}], "=", 
    FractionBox[
     RowBox[{"sin", 
      RowBox[{"(", 
       RowBox[{"j", "+", 
        RowBox[{
         FractionBox["1", "2"], "\[Alpha]"}]}], ")"}]}], 
     RowBox[{"sin", 
      RowBox[{"(", 
       RowBox[{
        FractionBox["1", "2"], "\[Alpha]"}], ")"}]}]]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"4a699e90-89fb-4211-ac80-e2971be90288"],
