Cell[TextData[{
 "With the help of the associate Legendre polynomials ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["P", "l", "m"], TraditionalForm]], "InlineMath",
  ExpressionUUID->"07db0852-d8ce-46b8-b83b-47d341bcb87b"],
 " (see ",
 ButtonBox["LegendreP",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/LegendreP"],
 ") the spherical harmonics can be calculated in cartesian form, with the \
help of the following equation:"
}], "Notes",
 Deletable->True,
 CellID->1308465110,ExpressionUUID->"be6e48c0-83d6-496d-92ba-82b6e6362f5b"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["Y", "l", "m"], "(", 
    RowBox[{"x", ",", "y", ",", "z"}], ")"}], "=", 
   RowBox[{
    SubsuperscriptBox["N", "l", "m"], 
    RowBox[{
     SubsuperscriptBox["P", "l", "m"], "(", 
     FractionBox["z", "r"], ")"}], 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{"|", "m", "|"}]},
         {"k"}
        }], ")"}], 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "k"], 
         SuperscriptBox["y", 
          RowBox[{"|", "m", "|", 
           RowBox[{"-", "k"}]}]]}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["x", "2"], "+", 
           SuperscriptBox["y", "2"]}], ")"}], 
         RowBox[{"|", "m", "|"}]]], "[", 
       RowBox[{
        RowBox[{"Cos", "(", 
         RowBox[{
          FractionBox[
           RowBox[{"|", "m", "|", 
            RowBox[{"-", "k"}]}], "2"], "\[Pi]"}], ")"}], "+", 
        RowBox[{"i", " ", 
         RowBox[{"sign", "(", "m", ")"}], " ", 
         RowBox[{"Sin", "(", 
          RowBox[{
           FractionBox[
            RowBox[{"|", "m", "|", 
             RowBox[{"-", "k"}]}], "2"], "\[Pi]"}], ")"}]}]}], "]"}]}]}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"8763c6b6-ee4f-48aa-b977-e6247c6e2c39"]], "Notes",\

 Deletable->True,
 CellID->1118597035,ExpressionUUID->"b99574ce-abab-4559-a227-713b5b6559ae"],

Cell[TextData[{
 "The result is normalized using the following pre-factor:",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["N", "l", "m"], "=", 
    SqrtBox[
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"2", "l"}], "+", "1"}], 
       RowBox[{"4", " ", "\[Pi]"}]], " ", "\[CenterDot]", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"l", "-", "m"}], ")"}], "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"l", "+", "m"}], ")"}], "!"}]]}]]}], TraditionalForm]], 
  "InlineMath",ExpressionUUID->"1a3d8dfc-7c9a-41e2-a743-04b510ac7a00"],
 "."
}], "Notes",
