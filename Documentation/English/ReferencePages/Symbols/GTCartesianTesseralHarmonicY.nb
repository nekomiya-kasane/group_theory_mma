Cell["\<\
The cartesian tesseral harmonics are implemented according to the definition \
of Podolskiy and Vogl. \
\>", "Notes",
 CellID->1054995823,ExpressionUUID->"b3671cf6-78b5-4f94-8bcf-76b7d89c766f"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["S", 
     StyleBox["l", "TI"], 
     StyleBox["m", "TI"]], "(", 
    StyleBox[
     RowBox[{"x", ",", "y", ",", "z"}], "TI"], ")"}], "=", 
   RowBox[{"\[Piecewise]", GridBox[{
      {
       RowBox[{
        SqrtBox["2"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "m"], "Re", " ", 
        SubsuperscriptBox["Y", "l", "m"], 
        RowBox[{"(", 
         RowBox[{"x", ",", "y", ",", "z"}], ")"}]}], 
       RowBox[{"m", ">", "0"}]},
      {
       RowBox[{
        SubsuperscriptBox["Y", "l", "m"], 
        RowBox[{"(", 
         RowBox[{"x", ",", "y", ",", "z"}], ")"}]}], 
       RowBox[{"m", "=", "0"}]},
      {
       RowBox[{
        SqrtBox["2"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "m"], "Im", " ", 
        SubsuperscriptBox["Y", "l", "m"], 
        RowBox[{"(", 
         RowBox[{"x", ",", "y", ",", "z"}], ")"}]}], 
       RowBox[{"m", "<", "0"}]}
     }]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"939d997b-ec63-47b3-aebb-c7a7fcda9e43"]], "Notes",\

