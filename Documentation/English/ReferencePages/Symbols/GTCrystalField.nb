Cell["\<\
The crystal field potential can be expanded into spherical harmonics.\
\>", "Notes",
 CellID->1170070192,ExpressionUUID->"5a96a3f9-cf17-4280-b145-92335398c30b"],

Cell[TextData[{
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["V", "cr"], "(", 
     RowBox[{"r", ",", "\[Theta]", ",", "\[Phi]"}], ")"}], "=", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"l", "=", "0"}], "\[Infinity]"], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"m", "=", 
        RowBox[{"-", "l"}]}], "l"], 
      RowBox[{
       SuperscriptBox["r", "l"], 
       SubsuperscriptBox["A", "l", "m"], 
       RowBox[{
        SubsuperscriptBox["Y", "l", "m"], "(", 
        RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}]}]}]}]}], TraditionalForm]],
   "InlineMath",ExpressionUUID->"3c17af22-a054-4259-bca1-68e479e154ed"]
}], "Notes",
 CellID->851727581,ExpressionUUID->"f6a32712-55f0-4002-84d3-e7528aab879a"],

