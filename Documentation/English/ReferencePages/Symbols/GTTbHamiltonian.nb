Cell[TextData[{
 "If we want to construct the Hamiltonian for an fcc transition metal like Cu \
 using ",
 StyleBox["s, ",
  FontSlant->"Italic"],
 "p",
 StyleBox[", d ",
  FontSlant->"Italic"],
 "electrons in the basis we will set the basis information as:"
}], "ExampleText",
 CellID->293064787,ExpressionUUID->"3c639f28-8e52-444f-a763-a318cda3b0cd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"basis", "=", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"\"\<Cu\>\"", ",", "1", ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1", ",", "2"}], "}"}]}], "}"}], "}"}]}], 
  ";"}]], "Input",
 CellLabel->"In[2]:=",
