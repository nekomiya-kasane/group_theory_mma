Cell[BoxData[
 RowBox[{
  RowBox[{"dos", "[", "energ_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"1", "/", "2"}], "/", "ss\[Sigma]"}], "/", "\[Pi]"}], " ", 
   RowBox[{"NIntegrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Cos", "[", 
       RowBox[{"t", " ", 
