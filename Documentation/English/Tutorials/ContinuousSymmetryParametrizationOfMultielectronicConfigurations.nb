 CellID->982922221,ExpressionUUID->"b09c007a-1001-4de3-bf9e-3072abc9fc2e"],

Cell["Calculate the symmetry projection.", "TechNoteMathCaption",
 CellID->1490455405,ExpressionUUID->"883ef10e-92cc-4e3c-b6d1-1b453acad202"],

Cell[BoxData[
 RowBox[{
  RowBox[{"projham", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"Length", "[", "Ogrp", "]"}]}], " ", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Orep", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ".", "ham", ".", 
        RowBox[{"ConjugateTranspose", "[", 
         RowBox[{"Orep", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "Ogrp", "]"}]}], "}"}]}], "]"}]}], "//", 
    "Simplify"}]}], ";"}]], "Input",
 CellLabel->"In[27]:=",
 CellID->1695568527,ExpressionUUID->"2de35487-f8ef-45c7-b88b-f83e7e28b691"],

Cell["Solve for the symmetry invariant terms.", "TechNoteMathCaption",
 CellID->1044847493,ExpressionUUID->"f104c9fb-8c48-4591-8c65-35800d105204"],

Cell[BoxData[
 RowBox[{
  RowBox[{"sol", "=", 
   RowBox[{"Quiet", "@", 
    RowBox[{
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"projham", "\[Equal]", "ham"}], ",", 
       RowBox[{"Flatten", "[", "ham", "]"}]}], "]"}], "[", 
     RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}]], "Input",
 CellLabel->"In[28]:=",
 CellID->59045409,ExpressionUUID->"db0b06ab-92cb-4918-bbed-36ff2bc46803"],

Cell["Calculate the final Hermitian Hamiltonian.", "TechNoteMathCaption",
