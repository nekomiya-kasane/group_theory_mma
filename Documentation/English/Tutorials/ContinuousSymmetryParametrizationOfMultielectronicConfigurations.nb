Cell[BoxData[
 RowBox[{
  RowBox[{"HamCombined", "=", 
   RowBox[{
    RowBox[{"ham", "/.", 
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{"i_", ",", "j_"}], "]"}], "\[Rule]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"i", ">", "j"}], ",", 
        RowBox[{"Conjugate", "[", 
         RowBox[{"u", "[", 
          RowBox[{"j", ",", "i"}], "]"}], "]"}], ",", 
        RowBox[{"u", "[", 
         RowBox[{"i", ",", "j"}], "]"}]}], "]"}]}]}], "/.", "sol"}]}], 
  ";"}]], "Input",
 CellLabel->"In[29]:=",
 CellID->1272222853,ExpressionUUID->"5489a7e5-8454-4998-8a87-ac4587152292"],

