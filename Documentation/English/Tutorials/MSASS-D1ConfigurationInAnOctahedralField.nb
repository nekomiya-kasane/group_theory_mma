Cell[TextData[{
 "The mSASS Hamiltonian is projected using the representation matrices of the \
angular momentum representation,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", " ", "=", " ", 
    RowBox[{
     UnderscriptBox["\[Sum]", 
      RowBox[{"g", "\[Element]", "G"}]], 
     RowBox[{
      RowBox[{
       SuperscriptBox["D", "J"], "(", "g", ")"}], " ", "H", " ", 
      SuperscriptBox["D", "J"], 
      SuperscriptBox[
       RowBox[{"(", "g", ")"}], 
       RowBox[{"-", "1"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "ab9b1cb7-9507-42a6-b242-adc55cd72bfa"],
 "\nThe right hand-side of the above equation is evaluated as follows."
}], "TechNoteMathCaption",
 CellID->288261355,ExpressionUUID->"866ee21c-b228-4d48-92cb-a476402feacf"],

Cell[BoxData[
 RowBox[{
  RowBox[{"projham", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"Length", "[", "Ogrp", "]"}]}], " ", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Orep", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ".", "ham", ".", 
        RowBox[{"Inverse", "[", 
         RowBox[{"Orep", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "Ogrp", "]"}]}], "}"}]}], "]"}]}], "//", 
    "Simplify"}]}], ";"}]], "Input",
 CellLabel->"In[15]:=",
 CellID->65676622,ExpressionUUID->"87f6378d-7f74-4245-8e49-0186f22468c7"],

